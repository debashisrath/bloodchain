<script type="text/javascript">
$(document).ready(function(){
	$('a').tooltip({placement: 'top'});
	$("#userMenu").hover(function(){
		$("#userMenu  ul:first").show();
	},function(){
		$("#userMenu  ul:first").hide();
	});
});
	$.extend(jQuery.easing,{
		easeBounceLogin:function(x,t,b,c,d){var s=1.70158;var p=0;var a=c;if(t==0) return b;if ((t/=d)==1) return b+c;if (!p) p=d*.3;if(a < Math.abs(c)){a=c;var s=p/4;}else var s=p/(2*Math.PI)*Math.asin(c/a);return a*Math.pow(2,-10*t)*Math.sin((t*d-s)*(2*Math.PI)/p)+c+b;},
		easeOutQuart:function(x,t,b,c,d){return -c*((t=t/d-1)*t*t*t-1)+b;}
	});
	$(document).ready(function(){
		$('#dropLoginPanel > a').click(function(e){
			if ($(this).hasClass('active'))
			{
				e.preventDefault();
				return;
			}
			e.preventDefault();
			e.stopPropagation();
			$('#floatinLoginPanel').css('border','1px solid #AAA').animate({height: 220, padding: 20}, 1000, "easeBounceLogin", function(){
				$('[name="Login[email]"]').focus();
			});
			$('[name="Login[email]"]').focus();
			$(this).addClass('active');
		});
		$('#floatinLoginPanel [name="Login[submit]"]').click(function(e){
			var login = 
			{
				username: $('#dropLoginPanel input[name="Login[email]"]'),
				password: $('#dropLoginPanel input[name="Login[password]"]')
			}; 
			
			if (login.username.val() == '' || login.password.val() == '')
			{
				if(login.username.val() == '')
					login.username.focus();
				else
					login.password.focus();
					
				$('#floatinLoginPanel').css('right',10).animate({right: 0}, 300, "easeBounceLogin");
				$('#errorLogin').show().html('Username and password cannot be left blank.');
				console.log("F");
				return false;
			}else
			{
				$('#errorLogin').hide();
				console.log("T");
				return true;
			}
			
		});
		$('#floatinLoginPanel').click(function(e){
			e.stopPropagation();
		});
		$('body').click(function(){
			$('#dropLoginPanel > a').removeClass('active');
			$('#floatinLoginPanel').stop(true).animate({height: 0, padding: 0, border: 0}, 0);
		});
	});
</script>
</html>
