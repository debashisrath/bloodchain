$(document).ready(function(){
	$("a.home").hover(function(){
		$(this).addClass("has-hover");
		$(this).find("img").attr("src","/assets/dbf3ff05/images/home_hover_icon.png");
	},function(){
		$(this).removeClass("has-hover");
		$(this).find("img").attr("src","/assets/dbf3ff05/images/home_icon.png");
	});
	$("li#dropdown_column1,li#dropdown_column2,li#dropdown_column3").hover(function(){
		$(this).find("a.drop").css({"background":"url(/assets/dbf3ff05/images/dropdown_icon_white.png) no-repeat right 13px","color":"#ffffff"});
	},function(){
		$(this).find("a.drop").css({"background":"url(\"https://s3.amazonaws.com/fws3/static/fwmain/images/drop.png\") no-repeat right 13px","color":"#000000"});
	});
	$("#userMenu").hover(function(){
		$("#userMenu  ul:first").show();
	},function(){
		$("#userMenu  ul:first").hide();
	});
	
	$("#dropdown_column1").hover(function(e){
		$('#drop_down1').stop(true, true).slideDown(200, "easeOutQuart");
	},function(e){
		$('#drop_down1').hide();
	});
	
	$("#dropdown_column2").hover(function(e){
		$('#drop_down2').stop(true, true).slideDown(200, "easeOutQuart");
	},function(e){
		$('#drop_down2').hide();
	});
	
	$("#dropdown_column3").hover(function(e){
		$('#drop_down3').stop(true, true).slideDown(200, "easeOutQuart");
	},function(e){
		$('#drop_down3').hide();
	});
});

</script>	<div class="container">
